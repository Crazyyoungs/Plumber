<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
    <Script file="MinimapButton.lua"/>

	<Button name="PlumberLandingPageMinimapButton" mixin="PlumberLandingPageMinimapButtonMixin" registerForClicks="LeftButtonUp, RightButtonUp" clampedToScreen="true" movable="true" parent="MinimapCluster" frameStrata="MEDIUM" hidden="true">
		<Size x="36" y="36"/>
		<Frames>
			<Frame parentKey="VisualContainer">
				<Size x="4" y="4"/>
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
				<Layers>
					<Layer level="ARTWORK">
						<Texture parentKey="BaseTexture">
							<Size x="64" y="64"/>
							<Anchors>
								<Anchor point="CENTER"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="OVERLAY">
						<Texture parentKey="HighlightTexture" hidden="true">
							<Anchors>
								<Anchor point="TOPLEFT" relativeKey="$parent.BaseTexture" relativePoint="TOPLEFT"/>
								<Anchor point="BOTTOMRIGHT" relativeKey="$parent.BaseTexture" relativePoint="BOTTOMRIGHT"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
			</Frame>

			<Frame parentKey="MouseoverFrame" propagateMouseInput="Both" hidden="true">
				<Size x="198" y="198"/>
				<Anchors>
					<Anchor point="CENTER"/>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
			<OnClick method="OnClick"/>
			<OnMouseDown method="OnMouseDown"/>
			<OnMouseUp method="OnMouseUp"/>
			<OnSizeChanged method="OnSizeChanged"/>
		</Scripts>
	</Button>
</Ui>
